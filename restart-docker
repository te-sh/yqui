git pull
docker-compose -f docker-compose.prod.yml build

COMPOSE_PROJECT_NAME=yqui PORT=8085
  docker-compose down && docker-compose --file docker-compose.prod.yml up -d


COMPOSE_PROJECT_NAME=test PORT=8800 \
  docker-compose down && docker-compose --file docker-compose.prod.yml up -d

docker system prune --volumes --force
